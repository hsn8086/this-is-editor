{
  "name": "this-is-editor",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn type-check && vite build && uv run main.py --debug",
    "build": "yarn build-onefile",
    "build-nodebug-dir": "yarn license-gen && yarn type-check && yarn build-only && yarn build-py-dir",
    "build-nodebug-onefile": "yarn license-gen && yarn type-check && yarn build-only && yarn build-py-onefile",
    "build-debug-dir": "yarn license-gen && yarn type-check && yarn build-only && yarn build-py-dbg-dir",
    "build-debug-onefile": "yarn license-gen && yarn type-check && yarn build-only && yarn build-py-dbg-onefile",
    "build-py-onefile": "uv run nuitka --standalone --onefile --assume-yes-for-downloads --include-data-dir=web=web --include-module=fastapi --include-module=uvicorn --windows-disable-console --output-dir=.dist main.py",
    "build-py-dir": "uv run nuitka --standalone --assume-yes-for-downloads --include-data-dir=web=web --include-module=fastapi --include-module=uvicorn --windows-disable-console --output-dir=.dist main.py",
    "build-py-dbg-dir": "uv run nuitka --standalone --assume-yes-for-downloads --include-data-dir=web=web --include-module=fastapi --include-module=uvicorn --lto=no --debugger --output-dir=.dist main.py",
    "build-py-dbg-onefile": "uv run nuitka --standalone --onefile --assume-yes-for-downloads --include-data-dir=web=web --include-module=fastapi --include-module=uvicorn --lto=no --debugger --output-dir=.dist main.py",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "license-gen": "uv run tools/gen_license.py",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "ace-builds": "^1.43.2",
    "ace-code": "^1.43.2",
    "ace-linters": "^1.8.3",
    "lodash": "^4.17.21",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.11",
    "vue3-ace-editor": "^2.2.4",
    "vuetify": "^3.9.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-vuetify": "^4.0.0",
    "license-checker": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-layouts-next": "^1.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.1"
  }
}