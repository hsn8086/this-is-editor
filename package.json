{
  "name": "this-is-editor",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev-all": "vite build && uv run main.py",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-py": "uv run nuitka --standalone --onefile --include-data-dir=web=web --include-module=fastapi --include-module=uvicorn --windows-disable-console main.py",
    "build-all": "run-p type-check \"build-only {@}\" build-py",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.1",
    "@codingame/monaco-jsonrpc": "^0.4.1",
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "@types/lodash": "^4.17.20",
    "ace-builds": "^1.43.2",
    "ace-code": "^1.43.2",
    "ace-linters": "^1.8.3",
    "ace-sql-linter": "^1.1.0",
    "codemirror": "^6.0.2",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.52.2",
    "normalize-url": "^8.0.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-codemirror": "^6.1.1",
    "vue3-ace-editor": "^2.2.4",
    "vuetify": "^3.9.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-vuetify": "^4.0.0",
    "npm-run-all2": "^7.0.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-layouts-next": "^1.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.1"
